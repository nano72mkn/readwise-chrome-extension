{
   "background": {
      "persistent": false,
      "scripts": [ "js/jquery/jquery.min.js", "js/purify.min.js", "js/rollbar.js", "src/bg/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon19.png",
      "default_title": "Readwise For amazon.co.jp"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "src/inject/inject.css" ],
      "matches": [ "*://read.amazon.co.jp/*", "*://*.readwise.io/*" ]
   }, {
      "all_frames": true,
      "js": [ "js/jquery/jquery.min.js", "js/js-cookie.js", "js/rollbar.js", "src/inject/inject.js" ],
      "matches": [ "*://read.amazon.co.jp/*", "*://*.readwise.io/*" ]
   } ],
   "content_security_policy": "script-src 'self'",
   "default_locale": "en",
   "description": "Use the Readwise Chrome Extension to sync your Kindle highlights and then review them daily.",
   "homepage_url": "https://readwise.io",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Readwise_for_amazon.co.jp",
   "permissions": [ "notifications", "cookies", "storage", "alarms", "contextMenus", "*://read.amazon.co.jp/*", "*://*.readwise.io/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.8.2",
   "web_accessible_resources": [ "src/inject/newLoadAllQuotes.js", "js/rollbar.js" ]
}
